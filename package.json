{
  "name": "raopoc",
  "version": "1.0.0",
  "description": "Bank customer onboarding application",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"sleep 2 && pnpm dev:frontend\"",
    "dev:frontend": "vite",
    "dev:server": "cd server && nest start --watch",
    "dev:https": "concurrently \"pnpm dev:server\" \"sleep 2 && pnpm dev:frontend\" \"sleep 2 && pnpm dev:caddy\"",
    "dev:caddy": "caddy run --config Caddyfile",
    "build": "tsc && vite build",
    "build:server": "cd server && nest build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "generate-pdfs": "tsx scripts/generate-pdfs.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/serve-static": "^5.0.3",
    "@types/express": "^5.0.3",
    "@vanilla-extract/css": "^1.17.2",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/vite-plugin": "^4.0.17",
    "chokidar": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "clsx": "^2.1.1",
    "express": "^5.1.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "lucide-react": "^0.468.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.2",
    "react-router": "^7.6.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/testing": "^11.1.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.0",
    "@types/pdfkit": "^0.13.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "2.1.9",
    "concurrently": "^8.2.2",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "pdfkit": "^0.17.1",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^5.4.10",
    "vitest": "^2.1.8"
  },
  "private": true
}
